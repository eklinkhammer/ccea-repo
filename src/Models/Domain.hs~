{-# LANGUAGE MultiParamTypeClasses #-}

module Models.Domain
  (

  ) where

class (Agent a, Scoring b, Actor c) => Domain a b c where
  getGlobalScore :: Domain a b c -> Double
  timestep :: Domain a b c -> Domain a b c
  getAgents :: Domain a b c -> [a]
  getScoring :: Domain a b c -> [b]
  getOtherActors :: Domain a b c -> [c]


